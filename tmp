CREATE TABLE ADMIN.HA_PARTICIPANT_REWARDS (
	SUB_NUMBER VARCHAR(20),
	SUBSCRIBER_ID VARCHAR(2),
	MEM_BIRTH_DATE DATE,
	MEM_GENDER VARCHAR(1),
	MEM_FIRST_NAME VARCHAR(50),
	MEM_FIRST_NAME_SEC VARCHAR(50),
	MEM_MIDDLE_INITIAL VARCHAR(1),
	MEM_LAST_NAME VARCHAR(50),
	MEM_LAST_NAME_SEC VARCHAR(50),
	MEM_COMPOSITE_KEY VARCHAR(70),
	SUB_SNN VARCHAR(4),
	GROUP_ANNIV DATE,
	ACCT_NUM VARCHAR(10),
	GRP_NUM VARCHAR(10),
	ORIG_EFF_DATE DATE,
	ACCT_DESC VARCHAR(254),
	PERSON_ID VARCHAR(10),
	HA_ENROLLED VARCHAR(1),
	DATE_ENROLLED DATE,
	REWARD_AMT_RECIEVED VARCHAR(5),
	DATE_REWARD_EARNED DATE,
	CALC_EE VARCHAR(1),
	ACTIVE_OR_TERMINATED VARCHAR(1),
	ACC_START_DATE DATE,
	ACC_END_DATE DATE,
	PARTICIPATION_PERIOD VARCHAR(4),
	RECORD_YYYYMM VARCHAR(6),
	DW_LOAD_DATE DATE
);
