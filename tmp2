
CREATE TABLE ADMIN.PRD_MST_CARR_CD_TBL (
	MASTER_CARRIER_CODE VARCHAR(4),
	MASTER_CARRIER_CODE_DESC VARCHAR(50),
	SOURCE_IND VARCHAR(3),
	ACTIVE_IND VARCHAR(1),
	LAST_MOD_DATE DATE
);


